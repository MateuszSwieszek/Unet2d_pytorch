pipeline {
    environment
    {
        REPO_PATH = "${WORKSPACE}/Unet2d_pytorch"
    }
    agent any
    stages {
        stage('Build') {
            steps {
                sh ''' echo "TESSSST" '''
                sh ''' echo  ${REPO_PATH}'''
                sh ''' ${REPO_PATH}/Jenkins/start_file.sh '''
                sh '''
                    echo "Multiline shell steps works too"
                    ls -lah
                '''
            }
        }
    }
}